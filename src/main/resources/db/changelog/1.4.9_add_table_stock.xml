<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="1.4.9" author="rivo pelu">
        <createTable tableName="stock">
            <!--START BASE ENTITY-->
            <column name="id" type="varchar(255)">
                <constraints nullable="false" unique="true" primaryKey="true"/>
            </column>
            <column name="active" type="tinyint" defaultValue="1">
                <constraints nullable="false"/>
            </column>
            <column name="created_by" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="created_date" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="deleted_by" type="varchar(255)"/>
            <column name="deleted_date" type="bigint"/>
            <column name="updated_by" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="updated_date" type="bigint">
                <constraints nullable="false"/>
            </column>
            <!-- END BASE ENTITY -->
            <column name="warehouse_id" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="unit_type_id" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="variant_product_id" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="quantity" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>